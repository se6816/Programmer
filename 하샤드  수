class Solution {
    public boolean solution(int x) {
        boolean answer = true;
        String[] st=String.valueOf(x).split("");
        int sum=0;
        for(int i=0;i<st.length;i++){
            
          sum+=Integer.parseInt(st[i]);
        }
        answer= x % sum == 0 ? true : false;
        
        return answer;
    }
}
