import java.util.*;
class Solution {
    public int[] solution(long n) {
        int[] answer = {};
        String x = Long.toString(n);
        int top=0;
        answer=new int[x.length()];
        for(int i=x.length()-1;0<=i;i--){
            answer[top++]=Integer.parseInt(x.substring(i,i+1));
        }
        return answer;
    }
}
